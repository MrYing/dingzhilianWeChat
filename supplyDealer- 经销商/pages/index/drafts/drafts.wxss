.consequence{
    width:710rpx;
    background-color:#fff;
    padding:0 20rpx;
    margin-top: 20rpx;
}

.order-num{
    height: 60rpx;
    line-height: 60rpx;
    border-bottom: 2rpx solid #e5e5e5;
}

.num1{
    float: left;
    font-size:26rpx;
}

.num2{
    float: right;
    font-size:24rpx;
    color:#999;
}

.order-detail{
    height: 220rpx;
    border-bottom:1rpx solid #e5e5e5;

}

.detail-image{
    width:336rpx;
    height: 180rpx;
    float:left;
    margin:20rpx 0;
}

.detail-noimage{
    width:336rpx;
    height: 180rpx;
    float:left;
    margin:20rpx 0;
}

.noimage-text{
    display: block;
    width:160rpx;
    height: 120rpx;
    border:1px solid #f2f2f2;
    color:#ccc;
    text-align: center;
    line-height: 120rpx;
    margin-top:30rpx;
}

.detail-title{
    width:310rpx;
    font-size:32rpx;
    float:right;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    text-align: left;
    margin-top:48rpx;
}

.detail-text{
    width:310rpx;
    float:right;
    font-size:26rpx;
    color:#666;
    text-align: left;
}


